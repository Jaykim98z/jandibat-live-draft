// server/.env.example
# 서버 설정
NODE_ENV=development
PORT=3000

# 데이터베이스 (나중에 MongoDB Atlas 설정 시)
MONGODB_URI=mongodb://localhost:27017/jandibat-draft
DB_NAME=jandibat-draft

# CORS 설정
CORS_ORIGIN=http://localhost:3001

# JWT 설정 (나중에 인증 구현 시)
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=24h

# SOOP API 설정
SOOP_API_BASE=https://bjapi.afreecatv.com/api

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
